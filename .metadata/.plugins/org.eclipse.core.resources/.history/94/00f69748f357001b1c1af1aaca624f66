
public class NumerosPrimos {
	
	static int n ;
	
	public NumerosPrimos(int n) {
		this.n = n;
	}
	
	public void setN(int n) {
		this.n = n;
	}
	
	public int getN() {
		return n;
	}

	public static void ehPrimo(NumerosPrimos numero){
		
		numero.setN(n);
		
		for (int  i = 2; i < n; i++) {
			if ( n % i == 0) {
				System.out.println("not a prime number");
			}else {
				System.out.println("prime number");
			}
		}
	}
}



